{{#md-fab-speed-dial md-direction='up'}}
  {{#md-fab-trigger}}
    {{#md-button aria-label="menu" class="md-fab"}}
      {{md-icon md-svg-src="assets/images/icons/border-color.svg"}}
    {{/md-button}}
  {{/md-fab-trigger}}

  {{#md-fab-actions}}
    {{#md-button aria-label="New expense" class="md-fab md-raised md-mini md-warn" action='newExpense'}}
      {{md-icon md-svg-src="assets/images/icons/minus.svg"}}
    {{/md-button}}
    {{#md-button aria-label="New revenue" class="md-fab md-raised md-mini md-primary" action='newRevenue'}}
      {{md-icon md-svg-src="assets/images/icons/plus.svg"}}
    {{/md-button}}
  {{/md-fab-actions}}
{{/md-fab-speed-dial}}
